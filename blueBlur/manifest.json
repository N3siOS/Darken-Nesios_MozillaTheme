{
  "description": "thème pour votre navigateur bleu avec dégrader et effet de flou",
  "manifest_version": 2,
  "author": "Nesios",
  "name": "blueBlur",
  "version": "0.4.4",
  "homepage_url": "https://github.com/N3siOS/myMozillaTheme",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
